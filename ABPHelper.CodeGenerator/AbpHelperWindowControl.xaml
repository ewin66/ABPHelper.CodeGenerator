<UserControl x:Class="ABPHelper.CodeGenerator.AbpHelperWindowControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:views="clr-namespace:ABPHelper.CodeGenerator.Views"
             xmlns:views1="clr-namespace:ABPHelper.CodeGenerator.Views"
             mc:Ignorable="d"
             d:DesignHeight="335" d:DesignWidth="290"
             Name="AbpHelper">
    <Grid>
        <TabControl>
            <TabItem Header="Generate Business">
                <views1:AddNewBusiness/>
            </TabItem>
            <TabItem Header="Generate ApplicationService Methods">
                <views1:AddNewServiceMethod/>
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>